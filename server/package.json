{
  "name": "racky-backend",
  "version": "1.0.0",
  "description": "Backend API for Racky marketplace management platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run test",
    "clean": "rm -rf dist",
    "migrate": "node scripts/migrate.js",
    "migrate:create": "node scripts/create-migration.js",
    "migrate:up": "node scripts/migrate.js --up",
    "migrate:down": "node scripts/migrate.js --down",
    "migrate:status": "node scripts/migration-status.js",
    "migrate:reset": "node scripts/migrate.js --reset --confirm",
    "migrate:validate": "node scripts/migrate.js --validate"
  },
  "keywords": [
    "express",
    "mongodb",
    "marketplace",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/module-alias": "^2.0.4",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.1",
    "openai": "^5.13.1",
    "stripe": "^18.4.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "_moduleAliases": {
    "@/common": "dist/common",
    "@/auth": "dist/modules/auth",
    "@/admin": "dist/modules/admin",
    "@/dashboard": "dist/modules/dashboard",
    "@/demo": "dist/modules/demo",
    "@/marketplaces": "dist/modules/marketplaces",
    "@/notifications": "dist/modules/notifications",
    "@/opportunities": "dist/modules/opportunities",
    "@/products": "dist/modules/products",
    "@/stores": "dist/modules/stores",
    "@/subscriptions": "dist/modules/subscriptions"
  }
}
